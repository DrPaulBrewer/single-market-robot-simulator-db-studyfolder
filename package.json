    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.7",
    "esdoc": "^1.1.0",
    "eslint": "^6.6.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^6.2.2",
    "should": "^13.2.3"
  },
  "dependencies": {
    "array-prefer": "^0.1.0",
