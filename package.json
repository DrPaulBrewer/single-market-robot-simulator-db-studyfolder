  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drpaulbrewer/single-market-robot-simulator-db-studyfolder"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "coveralls": "^3.0.7",
    "esdoc": "^1.1.0",
    "eslint": "^6.6.0"
  },
  "dependencies": {
    "array-prefer": "^0.1.0",
    "p-reduce": "^2.1.0"
  }
}
