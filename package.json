{
  "name": "single-market-robot-simulator-db-studyfolder",
  "version": "2.0.0",
  "description": "studyfolder base class for single-market-robot-simulator web apps",
  "main": "build/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel src --out-dir build ; done",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/**.js",
    "go": "npm run clean && npm run lint && npm run build"
  },
